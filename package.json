{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint:ts": "tslint",
    "gql-gen": "cross-env CODEGEN_NO_NAMESPACES=true gql-gen --schema http://localhost:8000/___graphql --template graphql-codegen-typescript-template --out ./src/typings/ \"./src/**/*.tsx\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@types/react-helmet": "^5.0.8",
    "gatsby": "^2.13.21",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-embed-snippet": "^4.1.2",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.5",
    "gatsby-transformer-sharp": "^2.2.3",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.2",
    "@types/typography": "^0.16.2",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-template": "^0.18.2",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "typography-theme-fairy-gates": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  }
}
